// -----------------------------------------------------------
// RSYNC SETTINGS
// -----------------------------------------------------------
//
//  shell   - Shell Option
//  flags   - Set one or more flags.
//  src     - Source path
//  dest    - Destination path
//  exclude - Exclude files from transfer.
//
// -----------------------------------------------------------

{
    "shell": "ssh",
    "flags": "az",
    "src": "/path/to/source",
    "dest": "/path/to/destination",
    "ignore": [
        ".DS_Store",
        ".gitignore",
        ".editorconfig",
        "node_modules",
        "package.json",
        "gone.js"
    ]
}
